@inject IJSRuntime jsRuntime

<pre>
    <code class="language-csharp">
        @Code
    </code>
</pre>

@code {
    [Parameter]
    public string Code{ get; set; }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if(firstRender){
            await jsRuntime.InvokeVoidAsync("Prism.highlightAll");
        }
            
    }
}


